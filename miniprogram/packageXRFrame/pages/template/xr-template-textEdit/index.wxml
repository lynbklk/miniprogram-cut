<xr-demo-viewer
  title="自定义文字案例"
  intro="文本显示与排版示例"
  hint="要求基础库 v3.3.3 及以上"
>
  <xr-template-textEdit
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
  />
</xr-demo-viewer>
